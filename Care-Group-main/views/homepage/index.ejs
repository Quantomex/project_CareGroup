<style>
  /* Reset default list styles */
  nav,
  ul,
  li {
    margin: 0;
    padding: 0;
    list-style: none;
  }

  /* Main navigation styles */
  nav ul {
    background-color: #f1f1f100;
    padding: 10px;
  }

  nav ul li {
    position: relative;
  }

  nav ul li a {
    display: block;
    padding: 8px 12px;
    border-bottom: 3px solid transparent;

  }

  /* Hover effect for anchor tags */
  nav ul li a:hover {
    /* Change color on hover */
    color: black;
    scale: 1.1;
  }

  /* Hide submenus by default */
  nav ul ul {
    display: none;
    position: absolute;
    top: 100%;
    left: 0;
    background-color: #ffffff;
  }

  /* Show submenu on parent hover */
  nav ul li:hover>ul {
    display: block;
  }

  /* Adjust submenu styles */
  nav ul ul li {
    width: 200px;
  }

  nav ul ul li a {
    padding: 8px 12px;
  }

  /* Add hover effect to submenu items */
  nav ul ul li:hover>ul {
    display: block;
    left: 100%;
    top: 0;
  }
</style>
