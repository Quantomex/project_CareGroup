<html data-wf-domain="www.pinwheelapi.com" data-wf-page="646cfb6f451fbd6532d1a056" data-wf-site="632be9850bc65a6eacc40d2f" lang="en">
<head>
  <meta charset="utf-8" />
  <meta content="width=device-width, initial-scale=1" name="viewport" />
  <link href="https://assets-global.website-files.com/632be9850bc65a6eacc40d2f/css/pinwheel-preview.c062e69bf.min.css" rel="stylesheet" type="text/css" />
  <style>
    :focus {
      outline: 0 !important;
    }
.tr-left-content{
  font-size: 20px;
  font-weight: 400;
  line-height: 27px;
  font-family: open sans;
  color: #000;
}
.tr-image-wrapper{
  width: 0;
}
.tl-year-inner{
  border: 3px solid #CE994B;
  padding: 0 40px !important;
  margin-bottom: 0;
}
.list li{
  list-style: disc !important;
  
}
  </style>
 
  <style>
    /**/
.tl-year{
  width: 0.1px;
  margin-top: 2px;
  border-right: #CE994B dotted 4px;
 
}

    /*css for body gradient*/
    .body.gradient-body {
      background-repeat: no-repeat;
    }

    .picture-grid .cards-navigation,
    .timeline-right .cards-navigation {
      order: 2;
    }
.tr-left-content{
  max-width: 100%;
}
    .timeline-right .cards-navigation {
      width: 100%;
      margin-top: 20px;
    }
.tl-year-text{
  font-size: 24px;
  font-weight: 400;
  font-family: open sans;
}
    .timeline-right .slick-list {
      margin-right: -10px;
    }
    .tr-inner-row{
      margin-bottom: 0;
    }

    .timeline-right .slick-list .slick-slide {
      margin-right: 10px;
    }

    @media (max-width: 768px) {

      .timeline-right,
      .tr-inner-row {
        display: flex !important;
        flex-direction: row !important;
      }

      .flex-2-image-row {
        display: flex !important;
      }
    }

    @media (max-width: 481px) {}
    
  </style>


  <section style="background-color: #af8b5510;" class="section-primary timeline-section">
   
    <div class="container-1040 w-container">
      <div data-w-id="345d458b-25f2-ca84-453f-58f4ae2b3bf7" style="opacity:0" class="timeline-inner">
        <div class="timeline-left">
            <% for (let image of story) { %>
          <a class="tl-year w-inline-block">
            <div class="tl-year-inner">
              <div class="tl-year-text"><%=image.date%></div>
            </div>
          </a>
          <% } %>
         </div>
        <div class="timeline-right">
            <% for (let images of story) { %>
          <div id="2018" class="tr-inner-row">
            <div class="tr-left">
              <div class="tr-mobile-title"><%=images.date%></div>
              <div class="tr-left-content"><%=images.description %></div>
            </div>
            <div class="tr-image-wrapper" style="width: 0;"></div>
          </div>
          <% } %>
        </div>
      </div>
    </div>
  </section>
 

  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=632be9850bc65a6eacc40d2f" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="https://assets-global.website-files.com/632be9850bc65a6eacc40d2f/js/pinwheel-preview.8b11da810.js" type="text/javascript"></script>





  <script type="text/javascript">
    (function(l) {
      if (!l) {
        window.lintrk = function(a, b) {
          window.lintrk.q.push([a, b])
        };
        window.lintrk.q = []
      }
      var s = document.getElementsByTagName("script")[0];
      var b = document.createElement("script");
      b.type = "text/javascript";
      b.async = true;
      b.src = "https://snap.licdn.com/li.lms-analytics/insight.min.js";
      s.parentNode.insertBefore(b, s);
    })(window.lintrk);
  </script>
 
  <script>
    if ($(window).width() < 768) {
      $('.timeline-right').slick({
        slidesToShow: 1,
        dots: true,
        arrows: true
      })
      $('.timeline-right .slick-dots, .timeline-right .slick-arrow').wrapAll('<div class="cards-navigation"></div>');
    }
    if ($(window).width() < 481) {
      $('.flex-2-image-row').slick({
        slidesToShow: 1,
        dots: true,
        arrows: true
      })
      $('.flex-2-image-row .slick-dots, .flex-2-image-row .slick-arrow').wrapAll('<div class="cards-navigation"></div>');
    }
    if ($(window).width() > 767) {
      var rowHeight = $('.tr-inner-row').outerHeight();
      var newHeight = rowHeight + 0;
      $('.tl-year').css('height', newHeight);
    }
  </script>
</body>

</html>

