<!-- Navbar Start  -->
<style>
  /* Reset styles for margin, padding, and list style */
  nav,
  ul,
  li {
    margin: 0;
    padding: 0;
    list-style: none;
  }
  
  /* Main navigation styles */
  nav ul {
    background-color: #f1f1f100;
    padding: 10px;
  }
  
  nav ul li {
    position: relative;
  }
  
  nav ul li a {
    text-align: center !important;
    display: block;
    padding: 8px 12px;
    border-bottom: 3px solid transparent;
    color: rgb(255, 255, 255); /* Default text color */
    text-decoration: none; /* Remove underlines */
  }
  
  /* Hover effect for anchor tags */
  nav ul li a:hover {
    /* Change color and scale on hover */
    color: #D0A25C; /* Change text color on hover */
    transform: scale(1.1);
  }
  nav ul li a:hover .svg-fill{
    /* Change color and scale on hover */
    fill: #D0A25C; /* Change text color on hover */
    transform: scale(1.1);
  }
  /* Hide submenus by default */
  nav ul ul {
    display: none;
    position: absolute;
    top: 100%;
    left: -20%;
    background: #d7b467;
    /* border-radius: 20px; */
    /* box-shadow: 1px 1px 6px #D0A25C; */
  }
  nav ul li a:active,
nav ul li a:focus {
  outline: none !important; /* Remove the default focus outline */
  border-bottom: 3px solid transparent !important; /* Remove the border on click */
}
.logo a:active,
.logo a:focus {
  outline: none !important; /* Remove the default focus outline */
  border-bottom: 3px solid transparent !important; /* Remove the border on click */
}
  /* Show submenu on parent hover */
  nav ul li:hover>ul {
    display: block;
  }
  
  /* Adjust submenu styles */
  nav ul ul li {
    width: 230px;
    margin: 0 20px;
  }
  
  nav ul ul li a {
    color: rgba(255, 255, 255, 0.858);
    font-weight: 600;
    font-size: 16px;
    text-align: left !important;
    margin-left: 12px !important;
    padding: 8px 10px;
  }
  nav ul li a:active{
    border: none !important;
  }
  
  /* Add hover effect to submenu items */
  nav ul ul li:hover>ul {
    display: block;
    width: 200px;
    left: 100%;
    top: 0;
  }/* Existing CSS *//* Mobile responsive styles */
/* Mobile responsive styles */
.svg-fill{
  fill: white;
}
.active2{
  fill: #ce994b !important;
}
@media (max-width: 768px) {
  .main-menu{
    text-align: left !important;
  }
  .main-menu ul {
    
    text-align: left !important;
    background-color: transparent;
    width: auto;
    position: static;
 
    display: none;
  }

  .main-menu.active ul {
    display: block;
  }

  .main-menu ul li {
    
    text-align: left !important;
    border-bottom: solid 1px rgb(255, 255, 255);
    width: 100%;
    text-align: left;
  }

  .main-menu ul li a {
    width: 100%;
  }

  .submenu1.active,
  .submenu2.active,
  .submenu3.active {
    display: block;
  }
}

  
