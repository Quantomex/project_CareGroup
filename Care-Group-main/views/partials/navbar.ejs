  <script>
document.addEventListener('DOMContentLoaded', function () {
  // Get references to the "Home" and "Group Companies" links
  const homeLink = document.getElementById('homeLink');
  const groupCompaniesLink = document.getElementById('groupCompaniesLink');

  // Add a click event listener to the "Group Companies" link
  groupCompaniesLink.addEventListener('click', function (e) {
    // Prevent the default behavior of the link
    e.preventDefault();

    // Remove the "active1" class from the "Home" link
    homeLink.classList.remove('active1');

    // Add the "active1" class to the "Group Companies" link
    groupCompaniesLink.classList.add('active1');

    // Navigate to the "Group Companies" section
    window.location.href = "/#groupcompani";
  });
});
</script>
